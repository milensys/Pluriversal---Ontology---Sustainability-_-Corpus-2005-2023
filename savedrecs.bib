@article{ WOS:000641101200001,
Author = {Squire, Rachael},
Title = {Where theories of terrain might land: Towards `pluriversal' engagements
   with terrain},
Journal = {DIALOGUES IN HUMAN GEOGRAPHY},
Year = {2021},
Volume = {11},
Number = {2},
Pages = {208-212},
Month = {JUL},
Abstract = {This brief commentary draws on Stuart Elden's thought provoking article,
   `Terrain, Politics, History', to make two interventions. The first
   argues that understandings of terrain and Earth's materiality would be
   enriched by engaging with `pluriversal' perspectives where Earth is
   inherently animated, lively, and heterogenous. This includes indigenous
   perspectives and those attending to non-human animal life. The second
   calls for a more concerted engagement within geography with terrains as
   voluminous constructs, whether that be in mountains, the air, or
   increasingly in artificial environments. It concludes by advocating for
   a further stretching of `terrain' as presented in the paper to see where
   diverse understandings of terrain might land or indeed, climb, float,
   sink, or soar.},
