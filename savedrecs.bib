@article{ WOS:000792150100001,
Author = {Querejazu, Amaya},
Title = {Cosmopraxis: Relational methods for a pluriversal IR},
Journal = {REVIEW OF INTERNATIONAL STUDIES},
Year = {2022},
Volume = {48},
Number = {5, SI},
Pages = {875-890},
Month = {DEC},
Abstract = {Bringing ontological questions back into IR has been key to opening
   discussions about plurality and difference in terms of the coexistence
   of related and plural worlds and realities, for example through notions
   such as relationality and pluriversality. The problem is that in trying
   to develop relational approaches as an alternative to the
   `Western/modern' - atomistic - ontology, relationality, relations, and
   their meanings can become fixed, translating them into `things'. The
   article maintains that cosmopraxis - as a complex pluriversal,
   multidimensional set of experiences - not only illustrates how relations
   relate without fixing their meaning, but also provides us with relevant
   insights to contribute to think of a pluriversal and more plural IR.},
Publisher = {CAMBRIDGE UNIV PRESS},
