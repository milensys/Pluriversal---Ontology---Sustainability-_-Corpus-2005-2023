@article{ WOS:000741798600001,
Author = {Mohlabane, Neo and Tshoaedi, Malehoko},
Title = {What Is a Woman? A Decolonial African Feminist Analysis of Womanhoods in
   Lesotho},
Journal = {SOUTH AFRICAN REVIEW OF SOCIOLOGY},
Year = {2021},
Volume = {52},
Number = {1},
