@article{ WOS:000822144700001,
Author = {Nayak, Bhabani Shankar},
Title = {Interdisciplinary limits of creative business education},
Journal = {TECHNOLOGICAL FORECASTING AND SOCIAL CHANGE},
Year = {2022},
Volume = {182},
Month = {SEP},
Abstract = {This article investigates the disciplinary denials of subjectivities and
   pluriversal nature of creative business education, examining the
   interdisciplinary limits of its curriculum produced in response to
   neoliberal crisis in business education. While creative business
   education is depicted as an alternative way out to regain the legitimacy
   of mainstream business school education and its dynamism, the programmes
   often recycled that upholds market values of compliant culture that
   destroys creativity of labour and the criticality of educational
   process. It highlights the continuity and growth of alienation within
   the interdisciplinary praxis of creative business education. It argues
   against the use of language, methods and conceptual narratives of
   traditional business education within creative business education for a
   radical transformation of business praxis that values people, planet and
   society.},
